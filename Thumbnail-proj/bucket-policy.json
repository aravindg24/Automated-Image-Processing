{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowCloudFrontOAI",
            "Effect": "Allow",
            "Principal": {
                "CanonicalUser": "<YOUR_CLOUDFRONT_OAI_CANONICAL_USER_ID>"
            },
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::<YOUR_PROCESSED_S3_BUCKET_NAME>/*"
        }
    ]
}