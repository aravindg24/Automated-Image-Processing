{
    "LambdaFunctionConfigurations": [
        {
            "Id": "ImageProcessingTrigger",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:626635405164:function:ImageProcessor",
            "Events": ["s3:ObjectCreated:*"],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "suffix",
                            "Value": ".jpg"
                        }
                    ]
                }
            }
        },
